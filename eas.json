{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false,
        "buildConfiguration": "Debug"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug",
        "resourceClass": "m-medium"
      }
    },
    "minimal": {
      "ios": {
        "simulator": true,
        "buildConfiguration": "Release",
        "resourceClass": "m-medium",
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "distribution": "store",
        "buildConfiguration": "Release"
      },
      "env": {
        "EAS_SKIP_AUTO_FINGERPRINT": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6755611796"
      }
    }
  }
}
